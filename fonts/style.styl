@import "variables"

@font-face
  font-family 'icomoon'
  src url(icomoon-font-path + '/icomoon.eot?ppmx7b')
  src url(icomoon-font-path + '/icomoon.eot?ppmx7b#iefix') format('embedded-opentype'), url(icomoon-font-path + '/icomoon.ttf?ppmx7b') format('truetype'), url(icomoon-font-path + '/icomoon.woff?ppmx7b') format('woff'), url(icomoon-font-path + '/icomoon.svg?ppmx7b#icomoon') format('svg')
  font-weight normal
  font-style normal

[class^="icon_"], [class*=" icon_"]
  font-family 'icomoon' !important
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1

  /* Better Font Rendering =========== */
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

